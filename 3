
class kvadrat1:
    def __init__(self, dlina, hirina):
        self.dlina = dlina       
        self.hirina = hirina
        print('kvadrat1',f'dlina:{self.dlina},nhirina:{self.hirina}')


class kvadrat2:
    def __init__(self, dlina, hirina):
        self.dlina = dlina       
        self.hirina = hirina
        print('kvadrat2',f'dlina:{self.dlina},nhirina:{self.hirina}') 

a = kvadrat1('36sm','12sm')
a = kvadrat2('23sm','15sm')
