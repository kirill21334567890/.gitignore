
class Class:
    def __init__(self, year, numbStudents):
        self.year = year
        self.numbStudents = numbStudents


    def show_my_drink(self)
        print(f'Год обучения : {self.year}')
        print(f'Количестыо учащихся: {self.numbStudents}')



class2 = Class('7','33')

 
class2.show_my_drink()

